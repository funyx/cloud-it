{
  "name": "cloud-it",
  "version": "1.0.0",
  "description": "cloud it server",
  "main": "index.js",
  "scripts": {
    "start": "tsc -w",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"yarn run watch-ts\" \"yarn run watch-node\"",
    "test": "mocha -r ts-node/register src/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funyx/cloud-it.git"
  },
  "author": "Dimitar Dobchev <ddobchev@gmail.com> (https://github.com/funyx)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/funyx/cloud-it/issues"
  },
  "homepage": "https://github.com/funyx/cloud-it#readme",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/socket.io": "^3.0.2",
    "concurrently": "^6.3.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.13",
    "socket.io": "^4.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "^16.11.1"
  }
}
